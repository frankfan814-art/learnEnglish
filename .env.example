# DeepSeek
DEEPSEEK_API_KEY=your_api_key_here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_ENDPOINT=https://api.deepseek.com/v1/chat/completions

# Provider: deepseek | ollama
LLM_PROVIDER=deepseek

# Use dataset as word source
USE_DATASET=1
DATASET_URL=https://huggingface.co/datasets/Maximax67/English-Valid-Words/resolve/main/valid_words_sorted_by_frequency.csv

# Dictionary (English -> Chinese)
DICT_URL=https://raw.githubusercontent.com/skywind3000/ECDICT/master/ecdict.csv

# Generation settings
TOTAL_WORDS=20000
EXAMPLES_PER_WORD=10
CHUNK_SIZE=500
MIN_LEN=6
MAX_LEN=14
